<script>
    function shareToSocialMedia(type) {
        const postUrl = encodeURIComponent(window.location.href);
        let socialLink = null;
        if (type === 0)
            socialLink = 'https://www.facebook.com/share_channel/?link=' + postUrl + '&app_id=966242223397117&source_surface=external_reshare&display&hashtag';
        else if (type === 0) {
            socialLink = 'https://x.com/intent/post?text=' + postUrl;
        } else if (type === 2) {
            socialLink = 'https://www.linkedin.com/feed/?linkOrigin=LI_BADGE&shareActive=true&shareUrl=' + postUrl;
        } else if (type === 3) {
            socialLink = 'https://www.instagram.com/?url=' + postUrl;
        }
        const newWindow = window.open(socialLink, '_blank');
        if (newWindow) {
            newWindow.focus();
        } else {
            alert('fail to share article to social media');
        }
    }
</script>
